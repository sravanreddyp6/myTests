    <validationRules>
        <fullName>Service_Changed_Yes</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK( IfYesWhat__c ),ISPICKVAL(   AmendmentChangesTermsRetroactively__c ,&apos;Yes&apos;))</errorConditionFormula>
