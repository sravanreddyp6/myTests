    <validationRules>
        <fullName>Service_Changed_No</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK( IfYesWhat__c )),ISPICKVAL(   AmendmentChangesTermsRetroactively__c ,&apos;No&apos;))</errorConditionFormula>
