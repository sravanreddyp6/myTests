        <fullName>VPO_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(VPO__c),OR(TEXT(  AreTheRatesChanging__c )=&apos;Lower&apos;,TEXT(  HaveTheAmountOfServicesChanged__c )=&apos;Increased&apos;))</errorConditionFormula>
