    <validationRules>
        <fullName>Admission_Status_IsActiveorDischarged</fullName>
        <active>true</active>
        <description>Admission status should be either Active or Discharged</description>
        <errorConditionFormula>OR(ISPICKVAL( Status__c , &quot;Completed&quot;),ISPICKVAL( Status__c , &quot;On-Hold&quot;),ISPICKVAL( Status__c , &quot;Referred Out&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please choose the status Active or Discharged</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discharge_Date_After_Intake_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Effective_Date__c &gt;  Discharged_Date__c</errorConditionFormula>
        <errorDisplayField>Discharged_Date__c</errorDisplayField>
        <errorMessage>Discharge Date must be greater than or equal to Effective Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discharged_Requirements</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Discharged&quot;),ISNULL(Discharged_Date__c))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>An Admission must have a Discharged Date before it can be discharged.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discharged_Requirements_2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Status__c, &quot;Discharged&quot;)),NOT(ISNULL(Discharged_Date__c)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status must be changed to Discharged when entering a Discharge Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referred_Out_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c , &quot;Referred Out&quot;),
OR(
ISBLANK(Referred_Out_Date__c),
ISBLANK(Referred_Out_Agency_Name__c),
ISBLANK(Referred_Out_Reason__c)
) 
)</errorConditionFormula>
