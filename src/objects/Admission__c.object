    <validationRules>
        <fullName>Discharge_Date_After_Intake_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Effective_Date__c &gt;  Discharged_Date__c</errorConditionFormula>
        <errorDisplayField>Discharged_Date__c</errorDisplayField>
        <errorMessage>Discharge Date must be greater than or equal to Effective Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discharged_Requirements</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Discharged&quot;),
OR(
ISNULL(Discharged_Date__c),
ISPICKVAL(Discharged_Status__c,&quot;&quot;),
ISBLANK(Reason_for_Discharge__c)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>An Admission must have a Discharged Date, Discharged Status, and Reason for Discharge, before it can be discharged.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discharged_Requirements_2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Status__c, &quot;Discharged&quot;)), 
OR( 
NOT(ISNULL(Discharged_Date__c)), 
NOT(ISPICKVAL(Discharged_Status__c,&quot;&quot;)), 
NOT(ISBLANK(Reason_for_Discharge__c))))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status must be changed to Discharged when entering a Discharge Date/Reason/Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referred_Out_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c , &quot;Referred Out&quot;),
OR(
ISBLANK(Referred_Out_Date__c),
ISBLANK(Referred_Out_Agency_Name__c),
ISBLANK(Referred_Out_Reason__c)
) 
)</errorConditionFormula>
        <errorMessage>For Admission records with a status of &quot;Referred Out&quot;, please include all referred out details below (i.e. data, agency name and reason).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Assignment_WNo_End_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Service_Assignments_With_No_End_Date__c  &gt; 0, OR(text( Status__c)=&quot;Completed&quot;,text( Status__c)=&quot;Discharged&quot;))</errorConditionFormula>
        <errorMessage>You cannot mark this record Completed or Discharged because there are Service Assignments that do not have an end date set. Please set end dates for all Service Assignments and try again.</errorMessage>
    </validationRules>
