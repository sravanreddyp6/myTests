    <validationRules>
        <fullName>Under_18_Requires_Guardian</fullName>
        <active>true</active>
        <description>Display an error message if the birthdate calculates to less than 18 years old, there is no Guardian on file, and for now, the username is not rory.delapaz - this should really be the integration account - to get past the upsert issue.</description>
        <errorConditionFormula>AND( 
(TODAY() - PersonBirthdate ) &lt; (18*365), 
Guardian1_Name__pc = &quot;&quot; ,
NOT($User.Username=&quot;integration@thementornetwork.com.prod&quot;)
)</errorConditionFormula>
        <errorMessage>Person Being Served is less than 18 years old. Please ensure you have added a name to the Guardian1 field.</errorMessage>
    </validationRules>
