        <formula>IF(ISNULL( First_Contact_Date__c),0, 1) 
+ 
IF(ISBLANK( First_Contact_Attempt_Notes__c), 0, 1) 
+ 
IF(ISNULL(Week_1_Follow_Up__c), 0, 1) 
+ 
IF(ISBLANK(Week_1_Follow_Up_Notes__c), 0, 1) 
+ 
IF(ISNULL( Week_2_Follow_Up__c), 0, 1) 
+ 
IF(ISBLANK( Week_2_Follow_Up_Notes__c), 0, 1) 
+ 
IF(ISNULL( Week_3_Follow_Up__c ),0, 1) 
+ 
IF(ISBLANK(Week_3_Follow_Up_Notes__c),0, 1) 
+ 
IF(ISNULL( Info_Packet_Provided_Date__c ),0, 1) 
+ 
IF(ISNULL( Application_Sent_Date__c ),0, 1) 
+ 
IF(ISNULL( Application_Received_Date__c ),0, 1) 
+ 
IF(ISNULL( Contact_Made__c ),0, 1) 
+ 
IF(Text( Application_Review_Status__c )=&quot;Approved&quot;,1, 0)</formula>
