        <formula>IF(ISNULL( Info_Packet_Provided_Date__c ), zRedX__c , zGreenChk__c ) &amp; &quot;Info Packet Provided&quot; 
&amp; 
IF(ISNULL( Application_Sent_Date__c ), zRedX__c , zGreenChk__c ) &amp; &quot;Application Sent&quot; 
&amp; 
IF(ISNULL( Application_Received_Date__c ), zRedX__c , zGreenChk__c ) &amp; &quot;Application Recieved&quot; 
&amp; 
IF(ISNULL( Contact_Made__c ), zRedX__c, zGreenChk__c) &amp; &quot;Contact Made&quot; 
&amp; 
IF(Text( Application_Review_Status__c ) = &quot;Approved&quot;, zGreenChk__c, zRedX__c ) &amp; &quot;Approved&quot;</formula>
