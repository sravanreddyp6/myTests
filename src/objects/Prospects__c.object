    </validationRules>
    <validationRules>
        <fullName>Allow_on_hold_only_if_screening_stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(Status__c, &quot;On hold - Prospect&quot;), 
ISPICKVAL( Status__c , &quot;On hold - Agency&quot;)), 
NOT(ISPICKVAL( Application_Review_Status__c , &quot;Approved&quot;)), $User.ProfileId &lt;&gt; &apos;00eU0000000myCw&apos; )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The “on hold” value can only be selected if a Prospect has reached screening stage (after the application is approved)</errorMessage>
    </validationRules>
    <validationRules>
