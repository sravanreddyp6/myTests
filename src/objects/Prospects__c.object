    <validationRules>
        <fullName>Allow_Nurturing_only_on_Inquiry_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Nurturing&quot;),ISPICKVAL(  Application_Review_Status__c , &quot;Approved&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
