    <validationRules>
        <fullName>Content_Application_Required</fullName>
        <active>true</active>
        <description>Content Application Required requires that content uploaded to the IT Salesforce Libraries with the IT-Systems Content Type must select an application from its list.</description>
        <errorConditionFormula>AND(ISPICKVAL( Application__c , &apos;&apos;),  RecordType.Name == &apos;Application&apos;)</errorConditionFormula>
        <errorDisplayField>Application__c</errorDisplayField>
        <errorMessage>You must select a value from the list.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Content_Control_Review_Date_Required</fullName>
        <active>true</active>
        <description>Content Control Review Date Required requires that content uploaded to Salesforce Libraries with the Control Content Type must select a a date to review the content to ensure compliance.</description>
        <errorConditionFormula>OR(AND(RecordType.Name == &apos;Control&apos;, ISBLANK(Control_Review_Date__c)), AND(NOT(ISBLANK(Control_Review_Date__c)),  RecordType.Name == &apos;Control&apos;,  Control_Review_Date__c &lt;= TODAY()))</errorConditionFormula>
        <errorDisplayField>Control_Review_Date__c</errorDisplayField>
        <errorMessage>Populate the date (use the date picker or enter a date formatted as mm/dd/yyyy) that this content needs to be reviewed for control requirements.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Content_Control_Type_Required</fullName>
        <active>true</active>
        <description>Content Control Required requires that content uploaded to Salesforce Libraries with the Control Content Type must select a control type from the picklist.</description>
        <errorConditionFormula>AND(ISBLANK(Control_Type__c),  RecordType.Name == &apos;Control&apos;)</errorConditionFormula>
        <errorDisplayField>Control_Type__c</errorDisplayField>
        <errorMessage>You must select one or more values from the list.</errorMessage>
    </validationRules>
</CustomObject>
