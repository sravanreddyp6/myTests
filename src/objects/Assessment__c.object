        <errorConditionFormula>ISPICKVAL( Interval__c , &apos;Quarterly&apos;) &amp;&amp;  ISBLANK( Quarter__c )</errorConditionFormula>
        <errorDisplayField>Quarter__c</errorDisplayField>
        <errorMessage>Quarter is required when &quot;Quarterly&quot; is selected from the Interval picklist.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assessment_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Assessment_Date__c ) &amp;&amp;
  LOWER(Assessment_Type__r.Name)=&apos;cans&apos;</errorConditionFormula>
        <errorDisplayField>Assessment_Date__c</errorDisplayField>
        <errorMessage>Assessment Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IntervalRequired</fullName>
        <active>true</active>
        <description>A rule to require selection of an interval for each new assessment.</description>
        <errorConditionFormula>Assessment_Type__r.Name == &apos;CANS&apos; &amp;&amp; ISPICKVAL(  Interval__c , &apos;&apos;)</errorConditionFormula>
        <errorMessage>You must select an Interval value to create a new CANS assessment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RISK_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>LOWER(TEXT(Status__c))=&apos;active final&apos; &amp;&amp;
ISBLANK(Approval_Date__c) &amp;&amp;
(LOWER(Assessment_Type__r.Name) =&apos;risk&apos; ||  ISPICKVAL( Type__c , &apos;ISSA&apos;))</errorConditionFormula>
        <errorDisplayField>Approval_Date__c</errorDisplayField>
        <errorMessage>Approval Date  is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TFC_Family_Vistas_Selection_Required</fullName>
        <active>true</active>
        <description>A validation rule that requires a selection be made from the TFC/Family Vistas picklist</description>
        <errorConditionFormula>Assessment_Type__r.Name == &apos;CANS&apos; &amp;&amp; NOT(OR(ISPICKVAL( TFC_Family_Vistas__c , &apos;Yes&apos;) ,ISPICKVAL( TFC_Family_Vistas__c , &apos;No&apos;) ))</errorConditionFormula>
        <errorDisplayField>TFC_Family_Vistas__c</errorDisplayField>
        <errorMessage>A value must be selected for &quot;&quot;TFC / Family Vistas&quot;&quot; when completing a CANS assessment.</errorMessage>
    </validationRules>
