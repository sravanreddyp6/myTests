    <validationRules>
        <fullName>Admission_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Admission__c) &amp;&amp; 

(
Case(TEXT(Type__c),
 &apos;FAD&apos;, 1,
 &apos;Immunization - ChilD&apos;, 1,
 &apos;Immunization - Adult&apos;, 1, 0
)=0
)</errorConditionFormula>
        <errorDisplayField>Admission__c</errorDisplayField>
        <errorMessage>An Admission is required for all Assessments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Questions_not_answered</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c)=&apos;Active Final&apos; &amp;&amp;
 AssessmentQuestionCount__c != AssessmentResponseCount__c</errorConditionFormula>
        <errorMessage>All questions for the assessment have not been answered. Please answer all questions prior to finalizing the assessment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AssessmentQuarterValidation</fullName>
        <active>true</active>
        <description>A rule to make sure that if user selects &quot;quarterly&quot; in interval picklist that they are forced to enter a quarter number in field.</description>
        <errorConditionFormula>ISPICKVAL( Interval__c , &apos;Quarterly&apos;) &amp;&amp;  ISBLANK( Quarter__c )</errorConditionFormula>
        <errorDisplayField>Quarter__c</errorDisplayField>
        <errorMessage>Quarter is required when &quot;Quarterly&quot; is selected from the Interval picklist.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assessment_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Assessment_Date__c ) &amp;&amp;
  LOWER(Assessment_Type__r.Name)=&apos;cans&apos;</errorConditionFormula>
        <errorDisplayField>Assessment_Date__c</errorDisplayField>
        <errorMessage>Assessment Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CANS_SerivceLine_Req</fullName>
        <active>false</active>
        <errorConditionFormula>Assessment_Type__r.Name == &apos;CANS&apos; &amp;&amp; ISPICKVAL(Service_Line__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Service_Line__c</errorDisplayField>
        <errorMessage>A value must be selected for Service Line when completing a CANS assessment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IntervalRequired</fullName>
        <active>true</active>
        <description>A rule to require selection of an interval for each new assessment.</description>
        <errorConditionFormula>Assessment_Type__r.Name == &apos;CANS&apos; &amp;&amp; ISPICKVAL(  Interval__c , &apos;&apos;)</errorConditionFormula>
        <errorMessage>You must select an Interval value to create a new CANS assessment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Locked</fullName>
        <active>true</active>
        <errorConditionFormula>Locked__c &amp;&amp; IF(PRIORVALUE(Locked__c),true,false) &amp;&amp;  !ISCHANGED( OwnerId )</errorConditionFormula>
        <errorMessage>Locked Assessments Can Not be Updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RISK_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>LOWER(TEXT(Status__c))=&apos;active final&apos; &amp;&amp;
ISBLANK(Approval_Date__c) &amp;&amp;
(LOWER(Assessment_Type__r.Name) =&apos;risk&apos; ||  ISPICKVAL( Type__c , &apos;ISSA&apos;))</errorConditionFormula>
        <errorDisplayField>Approval_Date__c</errorDisplayField>
        <errorMessage>Approval Date  is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TFC_Family_Vistas_Selection_Required</fullName>
        <active>false</active>
        <description>A validation rule that requires a selection be made from the TFC/Family Vistas picklist</description>
        <errorConditionFormula>Assessment_Type__r.Name == &apos;CANS&apos; &amp;&amp; NOT(OR(ISPICKVAL( TFC_Family_Vistas__c , &apos;Yes&apos;) ,ISPICKVAL( TFC_Family_Vistas__c , &apos;No&apos;) ))</errorConditionFormula>
        <errorDisplayField>TFC_Family_Vistas__c</errorDisplayField>
        <errorMessage>A value must be selected for &quot;&quot;TFC / Family Vistas&quot;&quot; when completing a CANS assessment.</errorMessage>
    </validationRules>
