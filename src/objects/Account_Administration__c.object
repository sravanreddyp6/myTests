        <label>Last Name</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>User Accounts</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>First_Name__c</customTabListAdditionalFields>
        <lookupDialogsAdditionalFields>First_Name__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>Activ__c</lookupDialogsAdditionalFields>
        <searchResultsAdditionalFields>First_Name__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Activ__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Update_with_No_Values</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Action__c) = &quot;Update&quot;,
Assign_State_Administrator__c = FALSE, 
Unassign_State_Administrator__c = FALSE,
ISPICKVAL(Role__c, &quot;&quot;),
ISPICKVAL(Profile__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Action__c</errorDisplayField>
        <errorMessage>You have chosen nothing to Update. Update either assignment of State Administrator, Role, or Profile.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UseAcct_AddRemoveRegionProgram</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND( NOT ISBLANK(TEXT(Region_Add_Remove__c)), OR ( ISBLANK(TEXT(Programs__c)), ISBLANK( RegionName__c ) )),
AND( NOT ISBLANK(TEXT(Programs__c)), OR ( ISBLANK(TEXT(Region_Add_Remove__c)), ISBLANK( RegionName__c ) )),
AND( NOT ISBLANK( RegionName__c ), OR ( ISBLANK(TEXT(Programs__c)), ISBLANK(TEXT( Region_Add_Remove__c)) )) )</errorConditionFormula>
        <errorDisplayField>Region_Add_Remove__c</errorDisplayField>
        <errorMessage>When adding/removing a user from a Region, all three fields must contain a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_ActionRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
AND(ISCHANGED( Profile__c), NOT(ISPICKVAL(Profile__c, &quot;&quot;))),
AND(ISCHANGED(Role__c),NOT(ISPICKVAL(Role__c, &quot;&quot;))),
AND(ISCHANGED(Assign_State_Administrator__c), Assign_State_Administrator__c &lt;&gt; FALSE),
AND(ISCHANGED(Unassign_State_Administrator__c), Unassign_State_Administrator__c &lt;&gt; FALSE)),  ISPICKVAL(Action__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Action__c</errorDisplayField>
        <errorMessage>You must select an action to perform for the fields requested.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Already_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ActionText__c = &quot;Activation&quot;,  TMN_User_Account__r.Salesforce_User_Account__r.IsActive = TRUE)</errorConditionFormula>
        <errorDisplayField>Action__c</errorDisplayField>
        <errorMessage>User Account associated with TMN User is already Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Any_Action_No_TMN_User</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(TMN_User_Account__c), 
OR(
NOT(ISPICKVAL(Action__c, &quot;&quot;)),
Assign_State_Administrator__c = TRUE, 
Unassign_State_Administrator__c = TRUE,
NOT(ISPICKVAL(Role__c, &quot;&quot;)),
NOT(ISPICKVAL(Profile__c, &quot;&quot;))
)
)</errorConditionFormula>
        <errorDisplayField>TMN_User_Account__c</errorDisplayField>
        <errorMessage>Please associate the appropriate TMN User Account before requesting any actions for that user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_AssignAndUnassign</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Assign_State_Administrator__c = TRUE, Unassign_State_Administrator__c = TRUE)</errorConditionFormula>
        <errorMessage>Assign State Administrator and Unassign State Administrator may not both be checked, please uncheck either or both</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Assign_State_Admin</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TMN_User_Account__c = &quot;&quot;, Assign_State_Administrator__c = TRUE
)</errorConditionFormula>
        <errorDisplayField>Assign_State_Administrator__c</errorDisplayField>
        <errorMessage>State Administrator Assignment cannot occur on non-existent User account. Please assign a TMN User and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Invalid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TMN_User_Account__c  = &quot;&quot;,
OR(
CONTAINS(TEXT(Action__c),&quot;Password Reset&quot;),
CONTAINS(TEXT(Action__c),&quot;Deactivation&quot;)
))</errorConditionFormula>
        <errorDisplayField>Action__c</errorDisplayField>
        <errorMessage>Selected Action cannot be made on a non-existing User Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Profile</fullName>
        <active>true</active>
        <description>Removed User.IsActive = Yes/No from error condition, because the user active field is not set to Yes/No before the validation cuts in. Also updated the TEXT(Role__c) Value to say not equal to null. Ryan J.</description>
        <errorConditionFormula>AND( TEXT(Profile__c) == &quot;&quot;, TEXT(Action__c) = &quot;Activation&quot;)</errorConditionFormula>
        <errorDisplayField>Profile__c</errorDisplayField>
        <errorMessage>Profile is required for User Account Activation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Role</fullName>
        <active>true</active>
        <description>Removed User.IsActive = Yes/No from error condition, because the user active field is not set to Yes/No before the validation cuts in. Also updated the TEXT(Role__c) Value to say not equal to null. Ryan J.</description>
        <errorConditionFormula>AND( TEXT( Role__c ) == &quot;&quot;, TEXT(Action__c) = &quot;Activation&quot;)</errorConditionFormula>
        <errorDisplayField>Role__c</errorDisplayField>
        <errorMessage>Role is required for User Account Activation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Route_To_Change</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND(ISBLANK( User_State_Administrator__c ), NOT ISBLANK(Route_To_Change_Info__c)),AND(NOT ISBLANK( User_State_Administrator__c ), NOT ISBLANK(Route_To_Change_Info__c)))</errorConditionFormula>
        <errorDisplayField>Route_To_Change_Info__c</errorDisplayField>
        <errorMessage>Both Route To Change Information and State Administrator selection must be entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_StateAdminRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
AND(ISCHANGED(Transfer_Prospects_To__c), Transfer_Prospects_To__c &lt;&gt; &quot;&quot;),  AND(ISCHANGED(Route_To_Change_Info__c), Route_To_Change_Info__c  &lt;&gt; &quot;&quot;), 
AND(ISCHANGED(Region_Add_Remove__c), ISPICKVAL(Region_Add_Remove__c, &quot;&quot;)),
AND(ISCHANGED(Region__c),   Region__c &lt;&gt; &quot;&quot;), 
AND(ISCHANGED(Programs__c), ISPICKVAL(Programs__c, &quot;&quot;))),  User_State_Administrator__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>User_State_Administrator__c</errorDisplayField>
        <errorMessage>Unable to perform action(s) without State Administrator filled in. State Administrator needs to be notified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_State_Admin_Assigned</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Assign_State_Administrator__c = TRUE,  State_Admin_Assignment__c = &quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>Assign_State_Administrator__c</errorDisplayField>
        <errorMessage>State Administrator is currently Assigned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_State_Admin_Unassign</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Unassign_State_Administrator__c = TRUE, State_Admin_Assignment__c = &quot;No&quot;)</errorConditionFormula>
        <errorDisplayField>Unassign_State_Administrator__c</errorDisplayField>
        <errorMessage>State Administrator is not currently Assigned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UserAcct_Unassign_State_Admin</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TMN_User_Account__c = &quot;&quot;,Unassign_State_Administrator__c = TRUE
)</errorConditionFormula>
        <errorDisplayField>Unassign_State_Administrator__c</errorDisplayField>
        <errorMessage>State Administrator Unassignment cannot occur on non-existent User account. Please assign a TMN User and try again.</errorMessage>
    </validationRules>
