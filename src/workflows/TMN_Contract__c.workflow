    <rules>
        <fullName>30 day Note</fullName>
        <active>true</active>
        <criteriaItems>
            <field>TMN_Contract__c.Status__c</field>
            <operation>equals</operation>
            <value>Active</value>
        </criteriaItems>
        <criteriaItems>
            <field>TMN_Contract__c.DaysRemaining__c</field>
            <operation>greaterThan</operation>
            <value>30</value>
        </criteriaItems>
        <description>Time; Note</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Contract_30_60_90_day_Note</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>TMN_Contract__c.Termination_Date__c</offsetFromField>
            <timeLength>-30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
