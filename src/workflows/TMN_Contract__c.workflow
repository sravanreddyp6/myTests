    <fieldUpdates>
        <fullName>ContractUpdateWFAnnualValue</fullName>
        <description>If the contract timeframe is over a year and the value type is stated - Avg the total contract days into an annual value</description>
        <field>ValueAnnual__c</field>
        <formula>IF(AND(CONTAINS(TEXT(ValueEstStated__c), &apos;Stated&apos;),TotalDays__c&gt;365) ,Value__c/(TotalDays__c+1) *365, Value__c )</formula>
        <name>ContractUpdateWFAnnualValue</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
