                                                    <apex:outputText >{!r.sl.Program__c}</apex:outputText>
                                                </apex:column>
                                                <apex:column headerValue="Setting">
                                                    <apex:outputText >{!r.sl.Setting__c}</apex:outputText>
                                                </apex:column>
                                                <apex:column headerValue="Service Value">
                                                    <apex:outputText >{!r.sl.Service_Value__c}</apex:outputText>
                                                </apex:column>
                                                <apex:column headerValue="Phone">
                                                    <apex:outputText >{!r.sl.Phone__c}</apex:outputText>
                                                </apex:column>
                                                <apex:column headerValue="Street">
                                                    <apex:outputText >{!r.sl.Street__c}</apex:outputText>
