            <apex:pageBlockSection title="Timeframe" columns="2" rendered="{!AND(!editMode,isNeuro)}">
                <apex:outputField value="{!CareMeridian_Contract__c.Start_Date__c}"/> 
                <apex:outputField value="{!CareMeridian_Contract__c.Termination_Notice__c}"/>
