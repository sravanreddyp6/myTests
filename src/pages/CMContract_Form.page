                                                </apex:column>
                                            </apex:pageblockTable>                                                                                                
                                        </apex:pageblockSection>                                    
                                    </apex:pageBlock>
                                </apex:outputPanel>


                            </apex:pageblockSection>
                        </apex:facet>
                    </apex:actionStatus>
                </apex:outputPanel>                            
                <apex:PageBlockTable value="{!locations}" var="location" id="refreshContractLocs">
                    <apex:column headerValue="Action" >
                            <apex:commandLink value="Delete" action="{!RemoveItem}" reRender="refreshContractLocs" status="st">
