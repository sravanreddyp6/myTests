<script>
j$ = jQuery.noConflict();
  
  
function showdio(dio, dio2){
    j$( '.'+dio ).slideDown( 1000);
    j$( '.'+dio2 ).slideUp( 10 );      
    
};

function hidedio(dio, dio2){
    j$( '.'+dio ).slideUp( 1000 ); 
    j$( '.'+dio2 ).slideDown( 10);  
    
};


  
</script>
    
</head>
<apex:outputPanel id="frmwrapper">


<apex:form id="myFrm" >

