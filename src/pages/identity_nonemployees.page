    <apex:stylesheet value="{!URLFOR($Resource.jquery, 'css/custom-theme/jquery-ui-1.8.21.custom.css')}"/>

    <style type="text/css">
        .custPopup{
            background-color: white;
            border-width: 2px;
            border-style: solid;
            z-index: 9997;
            left: 50%;
            padding:10px;
            position: absolute;
            /* These are the 3 css properties you will need to change so the popup 
            displays in the center of the screen. First set the width. Then set 
            margin-left to negative half of what the width is. You can add 
            the height property for a fixed size pop up if you want.*/
            width: 1000px;
            margin-left: -500px;
            top:10px;
        }
        .waitPopup{
            background-color: white;
            border-width: 2px;
            border-style: solid;
            z-index: 9999;
            left: 50%;
            padding:10px;
            position: absolute;
            width: 100px;
            margin-left: -50px;
            top:100px;
        }        
        .popupBackground{
            background-color:black;
            opacity: 0.20;
            filter: alpha(opacity = 20);
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            z-index: 9996;
        }
        .waitPopupBackground{
            background-color:black;
            opacity: 0.20;
            filter: alpha(opacity = 20);
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            z-index: 9998;            
        }
        .block {
            border-width: 10px 4px;
            border-style: solid;
            border-color: #066463;
            -webkit-border-radius: 8px;
            -moz-border-radius: 8px;
            border-radius: 8px;
            padding: 4px;
            margin-bottom: 12px;
            width: 230px;
        }                    
        .block-title {
            color: #000000;
            padding: 0 4px;
            font-size: 1.25em;
            font-weight: bold;
        }       
        .block-inner {
            color: #000000;
            padding: 0 4px;
            font-size: 1em;
            font-weight: bold;
        }
              
            .pbHeaderButton1 {
		    float: right;
		    position: relative;
		    top: 10px;
		    right: 10px;
		}  
             .pbHeaderButton2{
		    float: none;
		    position: relative;
		    top: 10px;
		    right: -300px;
		}                
    </style>
<script type="text/javascript" >
   var j$ = jQuery.noConflict(); 
             jQuery(window).load(function($) {
             if({!onloadDialog}){
             	 j$('[id$=selectDelegateId]').val('');
                  j$('[id*=DelegateOnBehalfOf]').dialog("open");
                }
        });
    
    j$(function() {
    j$('[id*=DelegateOnBehalfOf]').dialog({
      resizable: false,
      draggable: false,
      height:300,
      width : 500,
      modal: true,
      autoOpen: false,
      top: 400
    }).parent().appendTo(j$('form[id$=identityNonEmployee]'));
}); 
 
    function CloseDialog() {
    j$('[id*=DelegateOnBehalfOf]').dialog("close");
    }
    function changeDelegate() {
    	selectDelegate(j$('[id$=selectDelegateId]').val(),j$('[id$=selectDelegateId]').val());
    }
    
    function changeDelegate2(){
        if(j$('[id$=WorkforceOnBehalf]').val().length>1){
    	selectDelegate2(j$('[id$=WorkforceOnBehalf]').val(),j$('[id$=WorkforceOnBehalf]').val());
        }    
    }
    
    </script> 
    
    <apex:actionStatus id="loadingBusy" stopText="">
        <apex:facet name="start">
            <div>
                <div class="waitPopupBackground" />
                <div class="waitPopup">
                    <table border="0" width="100%" height="100%">
                        <tr>
                            <td align="center"><b>Please Wait</b></td>
                        </tr>
                        <tr>
                            <td align="center"><img src="{!$Resource.AjaxLoad}"/></td>
                        </tr>
                    </table>
                </div>
            </div>
        </apex:facet>
    </apex:actionStatus>

    <apex:panelGrid >                
        <apex:panelGroup >
