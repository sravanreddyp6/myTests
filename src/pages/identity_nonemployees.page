                <apex:commandButton value="Next" rerender="details" action="{!next}" disabled="{!!hasNext}" status="loadingBusy"/>
                <apex:commandButton value="Last Page" rerender="details" action="{!last}" disabled="{!!hasNext}" status="loadingBusy"/>
                 </div> 
                </apex:pageBlock>
