        			var decNotChecked = false;
        			$('input[id*=selectednonWorking]').each(function (index, element) {
						if($(element).is(':checked')){
							atOne = true;
						}
						if($(element).val() == 'Not explore integrated employment at this time' && $(element).is(':checked')){
