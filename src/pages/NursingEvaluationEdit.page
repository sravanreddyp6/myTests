                                            height: {!neuroannotation.Annotation_Height__c}
                                        }
                                    }]
                                });
                                </apex:repeat>
                                var neurocreateAnnotationElements = function (neuroAnnotations) {
                                    j$('.neuroannotation-number').remove();
                                    j$('table#neuroAnnotations tr').remove();
                                    j$.each(neuroAnnotations, function (index, neuroannotation) {
                                        var j$divEl = j$("<div class='neuroannotation-number' style='pointer-events:none; position: absolute; text-align: center; left: " + neuroannotation.shapes[0].geometry.x * 100 + "%; width: " + neuroannotation.shapes[0].geometry.width * 100 + "%; top: " + neuroannotation.shapes[0].geometry.y * 100 + "%; height: " + neuroannotation.shapes[0].geometry.height * 100 + "%;'></div>");
                                        j$nel.parent().append(j$divEl);
                                        var j$textEl = j$("<span style='line-height: " + j$divEl.height() + "px; color: red; font-size: 1.5em;'>" + (index + 1) + "</span>");
                                        j$divEl.append(j$textEl);
                                        j$("table#neuroAnnotations tbody").append("<tr><td>" + (index + 1) + "</td><td>" + neuroannotation.text + "</td></tr>");
                                    });
                                }
                                neurocreateAnnotationElements(anno.getAnnotations());
                                anno.addHandler('onAnnotationCreated', function (neuroannotation) {
                                    neurocreateAnnotationElements(anno.getAnnotations());
                                });
                                anno.addHandler('onAnnotationUpdated', function (neuroannotation) {
                                    neurocreateAnnotationElements(anno.getAnnotations());
                                });
                                anno.addHandler('onAnnotationRemoved', function (neuroannotation) {
                                    neurocreateAnnotationElements(anno.getAnnotations());
                                });

