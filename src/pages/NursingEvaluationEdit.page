        jQuery(document).ready(function ($) {



            handleFeedingChange = function () {


                // get the value
                var feedtypes = '';
                $('input[id*=feedingtypes]').each(function (index, element) {
                    if ( $(element).prop('checked') == true )
                        feedtypes += $(element).val() + ';';
                });

                $('input[id$=formula]').prop('disabled','disabled');
                $('input[id$=tuberate]').prop('disabled','disabled');
                $('input[id$=potype]').prop('disabled','disabled');
                $('input[id$=potexture]').prop('disabled','disabled');
                $('[id$=lipids]').prop('disabled','disabled');


                if ( feedtypes.indexOf( 'GT;' ) >= 0 ||
                     feedtypes.indexOf( 'JT;' ) >= 0 ||
                     feedtypes.indexOf( 'PEG;' ) >= 0 ||
                     feedtypes.indexOf( 'NGT;' ) >= 0 )
                 {

                        $('input[id$=formula]').removeAttr('disabled');
                        $('input[id$=tuberate]').removeAttr('disabled');

                 }
                if ( feedtypes.indexOf( 'PO;' ) >= 0  )
                 {
                        $('input[id$=potype]').removeAttr('disabled');
                        $('input[id$=potexture]').removeAttr('disabled');
                 }

                if ( feedtypes.indexOf( 'PPN;') >= 0 ||
                            feedtypes.indexOf( 'TPN;') >= 0 ) {
                        $('[id$=lipids]').removeAttr('disabled');
                }

            };


            handleBladder = function() {
                // get the value
                var bladdertypes = '';
                $('input[id*=bladderDetailCheck]').each(function (index, element) {
                    if ( $(element).prop('checked') == true )
                        bladdertypes += $(element).val() + ';';
                });

                $('input[id$=ifICP]').prop('disabled','disabled');
                $('input[id$=ifICPQ]').prop('disabled','disabled');
                $('input[id$=icpDescribe]').prop('disabled','disabled');
                $('input[id$=indwelling]').prop('disabled','disabled');

                if ( bladdertypes.indexOf( 'Indwelling') >= 0  ||
                     bladdertypes.indexOf( 'Suprapubic') >= 0 ) {
                    $('input[id$=indwelling]').removeAttr('disabled');
                } else {
                    $('input[id$=indwelling]').val('');
                    $('input[id$=undateindwelling]').removeAttr('checked');

                }


                if ( bladdertypes.indexOf('ICP') >= 0 ) {
                    $('input[id$=ifICP]').removeAttr('disabled');
                    $('input[id$=ifICPQ]').removeAttr('disabled');
                    $('input[id$=icpDescribe]').removeAttr('disabled');
                } else {
                    $('input[id$=ifICP]').val('');
                    $('input[id$=ifICPQ]').val('');
                    $('input[id$=icpDescribe]').val('');

                }

            };

            handleUrineClarity = function() {
                // get the value
                var urineclar = '';
                $('input[id*=urineClarity]').each(function (index, element) {
                    if ( $(element).prop('checked') == true )
                        urineclar += $(element).val() + ';';
                });

                if ( urineclar.indexOf('Other') >= 0 ) {
                    $('input[id$=UrineClarityOther]').removeAttr('disabled');
                } else {
                    $('input[id$=UrineClarityOther]').val('');
                    $('input[id$=UrineClarityOther]').prop('disabled', 'disabled');
                }
            }

            handleBowel = function() {
                // get the value
                var bow = '';
                $('input[id*=bowelAppliance]').each(function (index, element) {
                    if ( $(element).prop('checked') == true )
                        bow += $(element).val() + ';';
                });

                if ( bow.indexOf('Other') >= 0 ) {
                    $('input[id$=bowelApplianceOther]').removeAttr('disabled');
                } else {
                    $('input[id$=bowelApplianceOther]').val('');
                    $('input[id$=bowelApplianceOther]').prop('disabled', 'disabled');
                }
            }

            handleIncont  = function() {
                var incont = $('input[id$=bladderIncont]').prop('checked');
                if (incont) {
                    // enable the set of checkboxes in bladderDetailCheck
                    $('input[id*=bladderDetailCheck]').each(function (index, element) {
                        $(element).removeAttr('disabled');
                        });
                    $('input[id$=ageAppropriate]').removeAttr('disabled');

                } else {
                    // disable the set of checkboxes and uncheck them all
                    $('input[id*=bladderDetailCheck]').each(function (index, element) {
                        $(element).prop('disabled','disabled');
                        $(element).prop('checked', false);
                        });
                }
                handleBladder();
            };

            handleIncontBowel  = function() {
                var incont = $('input[id$=bowelIncont]').prop('checked');
                if (incont) {
                    // enable the set of checkboxes
                    $('input[id*=bowelAppliance]').each(function (index, element) {
                        $(element).removeAttr('disabled');

                        });

                } else {
                    // disable the set of checkboxes and uncheck them all
                    $('input[id*=bowelAppliance]').each(function (index, element) {
                        $(element).prop('disabled','disabled');
                        $(element).prop('checked', false);
                        });


                }
                handleBowel();
                };


            handleDialysis = function() {

                var ischecked = $('input[id$=dialysisCheck]').prop('checked');
                if (ischecked) {
                    // enable the set of checkboxes
                    $('input[id*=dialysisDOW]').each(function (index, element) {
                        $(element).removeAttr('disabled');
                        });

                } else {
                    // disable the set of checkboxes and uncheck them all
                    $('input[id*=dialysisDOW]').each(function (index, element) {
                        $(element).prop('disabled','disabled');
                        $(element).prop('checked', false);
                        });
                }
            }


            // if checkbox for None is selected, disables all others in group
            handleNone = function( ids ) {

                var theval = $('[id$=' + ids + 'none]').prop('checked');
                var subctrl = [];

                if ( ids == 'p' || ids == 'neuropain') {

                        ctrl =  $('[id$=' + ids + 'timeday]');
                        subctrl.push( ctrl);

                        ctrl =  $('[id$=' + ids + 'activity]');
                        subctrl.push( ctrl);

                        ctrl =  $('[id$=' + ids + 'position]');
                        subctrl.push( ctrl);
                } else {

                    $('input[id*=' + ids + ']').each( function(index, element) {
                        if ( element.value.indexOf('None') < 0 )
                            subctrl.push( $(element) );
                        else
                            theval = $(element).prop('checked');
                        });



                }
                var ctrl;
                if (theval == true ) {
                        $.each(subctrl, function (_, ctrl) {
                            ctrl.prop('checked', false );
                            ctrl.prop('disabled', 'disabled' );
                            enableDisableField(ctrl);
                            });
                } else {
                        $.each(subctrl, function (_, ctrl) {
                            ctrl.removeAttr('disabled');
                            enableDisableField(ctrl);
                            });

                }

            }

            handleNutrition = function () {
                handleFeedingChange();
                handleIncont();
                handleIncontBowel();
                handleUrineClarity();
                handleDialysis();
            };

            handleNeuro = function() {
                handleMood();
                enableFalls('{!Evaluation__c.Number_Falls__c}');
                handleNone('p');
                handleNone('palleve');
                handleNone('neuropain');
                handleNone('neuropainalleve');

            }

            handleWFLsChange = function () {

                // get the value
                if ( $('[id$=wfls]').val() == 'Yes') {
                    // set all other checkboxes to negative - invalidate all answers
                    // further assessment
                    var furtherind = $('[id$=furtherind]');
                    furtherind.prop('checked', false);
                    furtherind.attr('disabled','disabled');
                    enableDisableField(furtherind);


                    // mobility devices
                    $('[id$=wheelchair]').prop('checked', false).attr('disabled','disabled');
                    $('[id$=walker]').prop('checked', false).attr('disabled','disabled');
                    $('[id$=cane]').prop('checked', false).attr('disabled','disabled');
                    $('[id$=othermobility]').prop('checked', false).attr('disabled','disabled');
                    enableDisableField($('[id$=othermobility]'));

                    // other special equipment
                    $('[id$=specialequip]').val('').attr('disabled','disabled');
                    enableDisableField($('[id$=specialequip]'));


                    // reset sliders
                    $.each($("[id*=slider]"), function (key) {
                        var s = $(this).slider( "widget" );
                        s.slider("value", 1);
                        s.slider("disable");
                    });
                } else {
                // set all other checkboxes to negative - invalidate all answers
                    // further assessment
                    $('[id$=furtherind]').removeAttr('disabled');

                    // mobility devices
                    $('[id$=wheelchair]').removeAttr('disabled');
                    $('[id$=walker]').removeAttr('disabled');
                    $('[id$=cane]').removeAttr('disabled');
                    $('[id$=othermobility]').removeAttr('disabled');

                    // other special equipment
                    $('[id$=specialequip]').removeAttr('disabled');

                    // reset sliders
                    $.each($("[id*=slider]"), function (key) {
                        var s = $(this).slider( "widget" );
                        s.slider("enable");
                    });

                };

            };

            handleSoresAll = function() {
                handleSores( null, 'gums');
                handleSores( null, 'tongue');
                handleSores( null, 'lips');
                handleSores( null, 'cheeks');
                };

            handleSores = function (typeofSelect, typeofsores) {

                var fieldtoupdate = typeofsores + 'sores';
                var listtocheck = '';

                // called to initialize when tab is loaded - reset to saved values
                if ( typeofSelect == null ) {
                    if ( typeofsores == 'gums')
                        listtocheck = '{!Evaluation__c.Gums__c}';
                    if (typeofsores == 'tongue')
                        listtocheck = '{!Evaluation__c.Tongue__c}';
                    if (typeofsores == 'lips')
                        listtocheck = '{!Evaluation__c.Lips__c}';
                    if (typeofsores == 'cheeks')
                        listtocheck = '!Evaluation__c.Cheeks__c}';
                    }
                else {
                    if ( $(typeofSelect).val() == 'Sores' && $(typeofSelect).prop('checked'))
                        listtocheck = 'Sores';
                    else
                        listtocheck = '';
                    }

                if ( listtocheck.indexOf('Sores') >= 0 )
                    $('[id$='+fieldtoupdate+']').removeAttr('disabled');
                else
                    $('[id$='+fieldtoupdate+']').attr('disabled','disabled');

            };

            handleSliderChange = function (inputIdToUpdate) {

                var $inputToUpdate = $(document.getElementById(inputIdToUpdate));
                return function (value) {
                    // Assign the value to the hidden input from the the slider
                    $inputToUpdate.val(value);
                };
            };

             handleRancho = function () {

                var ranchoScore = $('[id$=ranchoValue]').val();

                // enable 'responds to' if rancho anything except 1
                if ( ranchoScore.indexOf('1') < 0 ) {
                    $('[id$=responds]').each( function(index, element) {
                        $(element).removeAttr('disabled');
                    });
                } else {
                    $('[id$=responds]').each( function(index, element) {
                        $(element).prop('disabled','disabled');
                        $(element).prop('checked', false);

                    });

                }

             };

             handleMood = function() {
                // if sad or flat, enable depression question
                var mood = '';
                $('input[id*=moodlist]').each(function (index, element) {
                    if ( $(element).prop('checked') == true )
                        mood += $(element).val() + ';';
                });

                if ( mood.indexOf('Sad') >=  0 || mood.indexOf('Flat') >=  0 ) {
                    $('[id$=responds]').each( function(index, element) {
                        $('[id$=sadflatHurt]').removeAttr('disabled');
                    });
                } else {

                    $('[id$=sadflatHurt]').prop('disabled', 'disabled').val('');
                    enableDisableField( $('[id$=sadflatHurt]'));
                }

             };


             var addOrModifyAnnotation = function (annotation, isNew) {
                if (isNew) {
                    if (annotation.Id !== undefined) {
                        return;  // So that annotations added at initial page loads are not duplicated
                    };
                    annotation.Id = null;
                }
                Visualforce.remoting.Manager.invokeAction(
                    '{!$RemoteAction.NursingEvaluationEditController.addOrModifyAnnotation}',
                    annotation.Id,
                    annotation.src,
                    annotation.shapes[0].geometry.x,
                    annotation.shapes[0].geometry.y,
                    annotation.shapes[0].geometry.width,
                    annotation.shapes[0].geometry.height,
                    annotation.shapes[0].type,
                    annotation.text,
                    '{!Evaluation__c.Id}',
                    function (result, event) {
                        if (event.status) {
                            annotation.Id = result.Id;
                        } else {
                            alert("Failed to update annotation! Error: " + event.message);
                        }
                    }
                );
            };
            anno.addHandler('onAnnotationCreated', function (annotation) {
                addOrModifyAnnotation(annotation, true);
            });
            anno.addHandler('onAnnotationUpdated', function (annotation) {
                addOrModifyAnnotation(annotation, false);
            });
            anno.addHandler("onAnnotationRemoved", function (annotation) {
                Visualforce.remoting.Manager.invokeAction(
                    '{!$RemoteAction.NursingEvaluationEditController.deleteAnnotation}',
                    annotation.Id,
                    function (result, event) {
                        if (!event.status) {
                            alert("Failed to delete annotation! Error: " + event.message);
                            anno.addAnnotation(annotation);
                        }
                    }
                );

            });

        String.prototype.endsWith = function (suffix) {
                return this.indexOf(suffix, this.length - suffix.length) !== -1;
            };

        $.fn.realVal = function() {
                // This function is here to put all the value-getting functionalities
                // in one place. This is because calling val() on a checkbox input
                // will only return the default value.
                var $obj = $(this);
                var val = $obj.val();
                var type = $obj.attr('type');
                if (type && type==='checkbox') {
                    return $obj.prop('checked') ? '1' : '0';
                } else {
                    return val;
                }
            };


     // Do the field disable/enable functionality here
            // When values are in map -> will be enabled when key is changed
            enableDisableField = function (element) {
                var $element = $(element);
                // For multi-picklist, apparently the data- passthrough also
                // exist in the selected and unselected checkboxes, which confuse
                // the script, so we check for their existence here.
                if ($element.attr('multiple') == 'multiple' && ($element.attr('id').endsWith('_selected') || $element.attr('id').endsWith('_unselected'))) {
                    return;
                }
                var rules = $element.data("rules");
                rules && $.each(rules, function (key, value) {
                    var $targets = [];
                    if (value instanceof Array) {
                        $.each(value, function (_, selector) {
                            $targets.push($(document.getElementById(selector)));
                        });
                    } else {
                        $targets.push($(document.getElementById(value)));
                    }
                    var val = $element.realVal();
                    if (val == key || (val instanceof Array && $.inArray(key, val)!=-1)) {
                        // Enable fields
                        $.each($targets, function (_, $target) {
                            if ($target.attr("multiple")) {
                                $target.parent().find("select").prop("disabled", false);
                            } else {
                                $target.prop("disabled", false);
                            }
                        });
                    } else {
                        // Disable fields
                        $.each($targets, function (_, $target) {
                            // We also need to revert the field(s) to their original state
                            if ($target.attr("multiple")) {
                                // Simulate removing all the selected options
                                $(document.getElementById($target.attr('id') + '_selected')).find('option').attr('selected', 'selected');
                                // Fake clicking the Remove button here
                                location.href = $target.parent().find("a[title=Remove]").attr("href");
                                $target.parent().find("select").prop("disabled", true);
                            } else {
                                $target.prop("disabled", true);
                                var type = $target.attr('type');
                                if (type && type==='checkbox') {
                                    $target.prop('checked', false);
                                } else {
                                    $target.val('');
                                }
                            }
                            $target.trigger('change');
                        });
                    }
                });


            }

     setUpFields = function () {
                $('body input').on("change", function (event) {
                    enableDisableField(event.target);
                });
                $('body select').on("change", function (event) {
                    enableDisableField(event.target);
                });
                $('body input').each(function (_, target) {
                    enableDisableField(target);
                });
                $('body select').each(function (_, target) {
                    enableDisableField(target);
                });
                /*$('.multiSelectPicklistCell a[title=Remove]').on('click', function () {
                    eval(this.href);
                });*/

                // disable age-based field
                if ( {!Evaluation__c.ServiceAssignment__r.Admission__r.Person_Being_Served__r.Age__c} <= 3 )
                    $('input[id$=headcircum]').removeAttr('disabled');
                else
                    $('input[id$=headcircum]').prop('disabled','disabled');


            }
            setUpFields();
            handleSoresAll();
            handleNutrition();
            handleNeuro();
            disablePain();


        });   //end "ready" function




     function getNextTab( currTab ) {
            var nextTab = currTab;

            if ( currTab == 'demographicsTab' || currTab == '')
                nextTab = 'diagnosticTab';
            else if( currTab == 'diagnosticTab')
                nextTab =  'cardioTab';
            //else if ( currTab == 'marTab' )
                //nextTab =     'cardioTab';
            else if ( currTab== 'cardioTab')
                nextTab = 'nutritionTab';
            else if ( currTab == 'nutritionTab')
                nextTab = 'skinBodyTab';
            else if ( currTab == 'skinBodyTab')
                nextTab = 'neuroTab';
            else if ( currTab == 'neuroTab')
                nextTab = 'orthoTab';
            else if ( currTab == 'orthoTab')
                nextTab = 'rehabTab';
            else if ( currTab == 'rehabTab')
                nextTab = 'psychoSocialTab';
            else if ( currTab == 'psychoSocialTab')
                nextTab = 'dischargePlanTab';

            return nextTab;
            }





    </script>
    <script type="text/javascript">

           var lbs;
           var oz;
           var kg;
           var ft;
           var inc;
           var cm;

         j$=jQuery.noConflict();
         function calculate(formField,type){

           if(type=='lbs'){
               var total=getlbsValue() + getOzValue();
               j$('[id$=kg]').val(total);
           }

           if(type=='oz'){
               var total=getlbsValue() + getOzValue();
               j$('[id$=kg]').val(total);
           }

           if(type=='kg'){
               kg=j$('[id$=kg]').val();
               var kgtolbs = kg*2.2046;
               j$('[id$=lbs]').val(kgtolbs);
               j$('[id$=oz]').val('0.0');
           }

           if(type=='ft'){
            var htotal = getftValue() + getincValue();
            j$('[id$=cm]').val(htotal);

           }

           if(type=='inc'){
            var htotal = getftValue() + getincValue();
            j$('[id$=cm]').val(htotal);

           }

           if(type=='cm'){
               cm=j$('[id$=cm]').val();
               var inches = cm*0.3937;
               var inchtoft = Math.floor(inches/12);
               var cmtoinc = Math.round(inches%12);
               j$('[id$=ft]').val(inchtoft);
               j$('[id$=inc]').val(cmtoinc);

        }

      }

        function getlbsValue(){
             lbs=j$('[id$=lbs]').val();
             var lbtokgs = 0;
             if(lbs!=null && lbs!='')
                 lbtokgs = lbs*0.453592;
             return lbtokgs;

        }
        function getOzValue(){
             oz=j$('[id$=oz]').val();
             var oztokg=0;
             if(oz!=null && oz!='')
                 oztokg = oz*0.0283495;
             return oztokg;

        }

        function getftValue(){
            ft=j$('[id$=ft]').val();
            var fttocm = 0;
            if(ft!= null && ft!='')
                fttocm = ft*30.48;
            return fttocm;


        }

        function getincValue(){
            inc=j$('[id$=inc]').val();
            var inctocm = 0;
            if(inc!= null && inc!='')
                inctocm = inc*2.54;
            return inctocm;


        }

       disablePain = function(){


            painval = j$('[id$=painYes]').val();
            if(painval == 'Yes'){

                j$('[id$=painSection]').show();
                setupAnno();

            }
            if(painval != 'Yes'){
                j$('[id$=painSection]').hide();


                }

        }

        function enableFalls( historyVal )  {

            if (historyVal != '')
                   j$('[id$=fallsDetail]').removeAttr('disabled');
            else {
                j$('[id$=fallsDetail]').val('');
                j$('[id$=fallsDetail]').prop('disabled','disabled');

            }

        }
     </script>

