<apex:page Controller="PlanOfCare" sidebar="false" readonly="true" cache="true" showheader="false" applyHtmlTag="false" applyBodyTag="false" standardStylesheets="false" title="Plan Of Care" 
renderAs="pdf" >
<html>
<head>

<style>
.labelCol,.label {
    margin: 10px;
    font-weight: bold;
    padding-right: 10px;
    padding-left: 10px;
}

body{background-repeat:repeat-x;background-position:left top;font-size:75%;font-family:'Arial','Helvetica',sans-serif;background-color:#fff}


tr td {
    border-collapse: collapse !important;
    border-bottom: solid thin !important;
    border-color: #ececec !important;
}

.pageHeader {
    font-size: 120%;
}

.reportHeader {
    font-size: 110%;
}

.reportHeader {
    font-size: 110%;
}
#signaturesTable tr th {border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0;}
#signaturesTable tr td { border-color: black !important; border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0;height: 2em;}
#signaturesTable {border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0; width: 80%;}
#safetyConcernTable {-fs-table-paginate: paginate;}
#safetyConcernTable th {border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0;}
#safetyConcernTable tr td { border-color: black !important; border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0;height: 2em;}
#safetyConcernTable {border-style:solid;border-width:1px;border-collapse:collapse;border-spacing:0; width: 80%;}

.mytbl{
    width: 100%;
    margin-bottom:10px;
    margin-top:10px;
    padding-left: 10px;
}

.newObjClass {
/* background-color: cornsilk; */
}
.innerLabel {
    width: 100px;
    font-weight: bold;
    text-align: right;
    padding-right: .5em;
}
.risk-section-subhead { 
    font-weight:bold;
    background-color: #99ccff;
    width: 100%;
}
</style>

<style type="text/css" media="print">
@page { 
margin:3.5em;

@top-center { content:element(header);
}

@bottom-left { content: element(footer);
}

}
div.header {
    padding: 10px;
    align: right;
    width: 100%;
    position: running(header);
}

div.footer {
    padding: 10px;
    position: running(footer);
}

.pagenumber:before {
    content: counter(page);
}

.pagecount:before {
    content: counter(pages);
}
</style>

    </head>
   
    <body>
<apex:messages style="color: red;"/>
    <div class="header">
        <table style="width: 100%;">
            <tr><th><span>{!pbsObj.FirstName}&nbsp;{!pbsObj.Middle_Name__c}&nbsp;{!pbsObj.LastName}</span></th>
                <th style="text-align: center;"><apex:outputLabel value=" {!pbsObj.Birthdate}" /></th>
                <th style="text-align: right; padding-right: 1.5em;"><apex:outputLabel value="Medicaid ID {!pbsObj.Medicaid_ID__c}" /></th></tr>
        </table>
    </div>
    <div style="width: 100%; text-align: center; padding-bottom: .5em;" class="footer">
        <b>Page</b> <span class="pagenumber"></span><b>&nbsp;of </b><span class="pagecount"></span>
    </div>
        
<apex:outputPanel layout="none" rendered="{!!errorPresent}">
            <table class="pageHeader" style="width: 100%;">
                <thead>
                    <tr>
                        <td class="labelCol" style="width: 30%; font-size: 80%;">{!pbsObj.Service_Assignment__r[0].Name}</td>
                        <td class="labelCol" style="text-align: center; vertical-align: top;">
                                <apex:outputLabel value="Plan of Care"/> <br/>
                                <apex:outputLabel value="(Authorized Service Plan Addendum)"/></td>
                        <td style="width: 30%; text-align: right;">
                        </td>
                    </tr>
                </thead>
            </table>
            <table class="reportHeader" style="width: 100%;">
                <thead class="reportHeaderTableHeader">
                    <tr>
                        <td class="labelCol" style="text-align: center;"><apex:outputLabel value="Date of Plan of Care (POC) meeting:__/__/____" /></td>
                    </tr>
                    <tr>
                        <td class="labelCol" style="text-align: center;"><apex:outputLabel value="Effective Date of Approved Plan of Care:__/__/____" /></td>
                    </tr>
                </thead>
            </table>
            
            <br/>
            <apex:outputPanel layout="none">
              <table style="width: 100%" >
                    <tr>
                        <th colspan="2" class="risk-section-subhead">Services Needs/Support</th>
                    </tr>
                <apex:outputPanel rendered="{!assessment.Dsc_Prevocational_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Prevocational</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Prevocational_Services__c}"/></td>
                    </tr>
                 </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Day_Habilitation_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Day Habilitation</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Day_Habilitation_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Organizational_Employment_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Organizational Employment</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Organizational_Employment_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_School_Transition_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">School Transition</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_School_Transition_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Private_Pay_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Private Pay</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Private_Pay_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!AND(ISBLANK(assessment.Dsc_Private_Pay_Services__c), ISBLANK(assessment.Dsc_School_Transition_Services__c), ISBLANK(assessment.Dsc_Organizational_Employment_Services__c),
                                                  ISBLANK(assessment.Dsc_Day_Habilitation_Services__c), ISBLANK(assessment.Dsc_Prevocational_Services__c)) }" layout="none" >
                    <tr>
                        <td colspan="2" style="font: bold; padding-top: .5em;">None</td>
                    </tr>
                </apex:outputPanel>
              </table>                
            </apex:outputPanel>

            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Staffing/Supervision Needs</th>
                    </tr>
                    <apex:repeat value="{!Staffing_Supervision_Needs}" var="item">
                        <tr>
                            <td style="padding-top: .5em;"><apex:outputText value="{! SUBSTITUTE(item, '__', ' ')}"/></td>
                        </tr>
                    </apex:repeat>
                </table>
            </apex:outputPanel> 

            <br />
            <apex:outputPanel layout="none">
            	<table style="width: 100%" > 
                     <tr> 
                         <th colspan="2" class="risk-section-subhead">Comprehensive Functional Assessment Review</th>
                     </tr> 
                </table><br/>
               <b>Current strengths include but are not limited to</b><br/>
                <apex:outputText value="{!assessment.Strengths_long_text__c}"/> <br/><br/>
               <b>Current Needs include but not limited to</b><br/>
               <apex:outputText value="{!assessment.Needs_long_text__c}"/>
            </apex:outputPanel>
            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Summary of Consumer Incident Reports</th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;"><apex:outputText value="Please see Incident Report Attachment."/></td>
                    </tr>
                </table>
            </apex:outputPanel>

<!--             <div style="page-break-before: always;"></div> -->
            <br /><br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Goals (Goals the team determined is appropriate)</th>
                    </tr>
                    <tr>
                        <td style="width: 100%">
                            <apex:repeat value="{!planStuff}" var="g">    
                            <table class="mytbl">
                            <tr>
                                <td style="width: 100px;">        
                                    <apex:outputText ><b>Goal</b></apex:outputText>
                                </td>                
                                <td>
                                    <apex:outputPanel layout="block" style="padding-bottom:20px">
                                        <apex:outputText value="{!planStuff[g].theGoal.Description__c}" />
                                    </apex:outputPanel>            
                                </td>            
                            </tr>
                                    <apex:repeat value="{!planStuff[g].GoalObjs}" var="o" >
                            <tr>
                                            <td style="text-indent: 1em;">
                                                <apex:outputText ><b>Objective</b></apex:outputText>
                                            </td>                    
                                            <td>
                                                <apex:outputPanel layout="block" id="theObj" style="padding-bottom:.5em;" styleClass="newObjClass">
                                                    <apex:outputText value="{!planStuff[g].GoalObjs[o].theObj.Description__c}"/> 

                                                    <table style="width: 100%;">
                                                    <tr>
                                                        <td class="innerLabel" style="width: 10%"><apex:outputLabel value="Status" /></td>
                                                        <td style="width: 12%"><apex:outputText value="{!planStuff[g].GoalObjs[o].theObj.Status__c}" /></td>
                                                        <td class="innerLabel" style="width: 25%" ><apex:outputLabel value="Effective Date" /></td>
                                                        <td style="width: 14%"><apex:outputText value=" {!planStuff[g].GoalObjs[o].theObj.Effective_Date__c}" /></td>
                                                        <td class="innerLabel" style="width: 25%"><apex:outputLabel value="Target Date" /></td>
                                                        <td style="width: 14%"><apex:outputText value=" {!planStuff[g].GoalObjs[o].theObj.Target_End_Date__c}" /></td>
                                                    </tr>
                                                    </table> 
                                                                        
                                                </apex:outputPanel>
                                            </td>
                            </tr>                
                                            <apex:repeat value="{!planStuff[g].GoalObjs[o].ObjActs}" var="a" id="actsthe">
                            <tr class="action_row">
                                                    <td style="text-indent: 2.5em;">
                                                    <apex:outputText ><b>Action</b></apex:outputText>                
                                                    </td> 
                                                    <td>
                                                        <apex:outputPanel layout="block" id="theAction" style="padding-bottom:20px">
                                                            <apex:outputField value="{!planStuff[g].GoalObjs[o].ObjActs[a].theAction.Description__c}"/>                                          
                                                        </apex:outputPanel>
                                                    </td>
                            </tr>                    
                                        </apex:repeat> <!-- end action-->
                                </apex:repeat> <!-- end obj -->
                                </table>
                                <hr />
                            </apex:repeat>
                    
                        </td>
                    </tr>
                </table>
            </apex:outputPanel> 
            
<!--             <div style="page-break-before: always;"></div> -->
            <br /><br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Medical information/Support</th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">Does the individual have any specific dietary needs?</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Presents a Risk" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}" />
                                        <br/>
                                        <apex:outputText value="Plans to reduce risk:{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Plan_to_reduce_Risk__c}" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}" />
                                        <apex:outputText value="Does Not Present a Risk" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Does Not Present a Risk'}"/>
                                    </td>
                                </tr>
                            </table>

                        </td>
                    </tr>
                    
            <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}">
                    <tr>
                        <td>
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">If 'Presents a Risk', do the dietary needs address a significant health or safety risk to the individual?</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Yes, refer to DAO for instructions and completion" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Description_of_Risk__c == 'Yes, refer to DAO for instructions and completion'}" />
                                        <apex:outputText value="No, DAO not required per IDT recommendation" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Description_of_Risk__c == 'No, DAO not required per IDT recommendation'}" /> 
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
            </apex:outputPanel>

                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">Does the Individual require PRN During Day Programming Hours?</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Presents a Risk" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Presents a Risk'}" />
                                        <br/>
                                        <apex:outputText value="Plans to reduce risk:{!responseMap['Medications~PRN During Day Programming Hours'].Plan_to_reduce_Risk__c}" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Presents a Risk'}" />
                                        <apex:outputText value="Does Not Present a Risk" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Does Not Present a Risk'}" /> 
                                    </td>
                                </tr>
                            </table>

                        </td>
                    </tr>
                    
            <apex:outputPanel layout="none" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Presents a Risk'}">
                    <tr>
                        <td>
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">If 'Presents a Risk', Describe signs or symptoms that would require the need for the PRN to be administered</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Individual requests the medication" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Description_of_Risk__c == 'Individual requests the medication'}" />
                                        <apex:outputText value="CMA/MM observes the need based on the following symptoms:" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Description_of_Risk__c == 'CMA/MM observes the need based on the following symptoms:'}" /> 
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
            </apex:outputPanel>

                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">Does this individual Self-Medicate?</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Presents a Risk" rendered="{!responseMap['Medications~Self-Medicate'].Rating__c == 'Presents a Risk'}" />
                                        <br/>
                                        <apex:outputText value="Plans to reduce Risk:{!responseMap['Medications~Self-Medicate'].Plan_to_reduce_risk__c}" rendered="{!responseMap['Medications~Self-Medicate'].Rating__c == 'Presents a Risk'}" />
                                        <apex:outputText value="Does Not Present a Risk" rendered="{!responseMap['Medications~Self-Medicate'].Rating__c == 'Does Not Present a Risk'}" /> 
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    
                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">Can this individual transport medication to and from the program?</th>
                                </tr>
                                <tr>
                                    <td style="vertical-align: top; padding-left: 1em; ">
                                        <apex:outputText value="Presents a Risk" rendered="{!responseMap['Medications~Self-Transportation of Medication'].Rating__c == 'Presents a Risk'}" />
                                        <br/>
                                        <apex:outputText value="Plans to reduce risk:{!responseMap['Medications~Self-Transportation of Medication'].Plan_to_reduce_Risk__c} " rendered="{!responseMap['Medications~Self-Transportation of Medication'].Rating__c == 'Presents a Risk'}" />
                                        <apex:outputText value="No" rendered="{!responseMap['Medications~Self-Transportation of Medication'].Rating__c == 'Does Not Present a Risk'}" /> 
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>

                </table>
            </apex:outputPanel>

            <br /><br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Individual Rights Review <br/><sup>(This section will provide further detailed information on the individual's rights restrictions as noted in the authorized service plan)</sup></th>
                    </tr>
                </table>
                <b>Has this individual made any achievement towards their rights restriction restoration plans(lifting rights restrictions)? - {!assessment.Individual_Rights_Review__c}</b><br/>
                <apex:outputText rendered="{!assessment.Individual_Rights_Review__c == 'Yes'}"><b>Explain</b> <br/></apex:outputText>
                <apex:outputText value="{!assessment.Individual_Rights_Review_long_text__c}" rendered="{!assessment.Individual_Rights_Review__c == 'Yes'}" />
            </apex:outputPanel>

            <div style="page-break-before: always;"></div>
            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Safety Plan <br/><sup>(Please address the following safety concerns: fire, tornado and emergency. In addition, address any safety concerns specific to the consumer including but not limited to: seizures, diabetic emergencies, allergies(food, environmental, seasonal, etc.), stranger danger, wandering, elopement, behavioral etc. and what staff's actions would be)</sup></th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%;" id="safetyConcernTable" >
                              <thead>
                                <tr>
                                    <th style="width: 20%;">Safety Concern</th>
                                    <th style="width: 10%; text-align: center;">Presents a Risk</th>
                                    <th style="width: 40%;">Description</th>
                                    <th style="width: 30%;">Plan to Reduce Risk</th>
                                </tr>
                              </thead>
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>FIRE</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Behavioral~Fire starter'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Behavioral~Fire starter'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Behavioral~Fire starter'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Behavioral~Fire starter'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              </apex:outputPanel>
                              
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>TORNADO</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['General Environment~Ability to respond to weather related conditions'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['General Environment~Ability to respond to weather related conditions'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['General Environment~Ability to respond to weather related conditions'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['General Environment~Ability to respond to weather related conditions'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              </apex:outputPanel>
                              
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>EMERGENCY</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Personal~Responds to emergency situations'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Personal~Responds to emergency situations'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Personal~Responds to emergency situations'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Personal~Responds to emergency situations'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              </apex:outputPanel>
                              
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>WATER SAFETY</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Community~Demonstrates water safety skills'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Community~Demonstrates water safety skills'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Community~Demonstrates water safety skills'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Community~Demonstrates water safety skills'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              </apex:outputPanel>                          
    
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>FALL RISK-Balance</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Physical Limitations~Balance'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Physical Limitations~Balance'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Balance'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Balance'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              <tr>
                                <td>FALL RISK-Self Mobility</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Physical Limitations~Self Mobility'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Physical Limitations~Self Mobility'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Self Mobility'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Self Mobility'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
                              <tr>
                                <td>FALL RISK-Ambulatory</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Physical Limitations~Ambulatory'].Rating__c == 'Presents a Risk'}" /> 
                                <apex:outputText value="No" rendered="{!responseMap['Physical Limitations~Ambulatory'].Rating__c != 'Presents a Risk'}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Ambulatory'].Description_of_Risk__c}" /> </td>
                                <td><apex:outputText value="{!responseMap['Physical Limitations~Ambulatory'].Plan_to_reduce_Risk__c}" /></td>
                              </tr>
    
                              </apex:outputPanel>
                              
                              <apex:outputPanel layout="none">
                              <tr>
                                <td>SEXUAL ACTIVITY</td>
                                <td><apex:outputText value="Yes" rendered="{!responseMap['Community~Exhibits socially accepted behaviors in public'].Rating__c == 'Presents a Risk'}" /> 
