<apex:page controller="nonemployeeAttestationPageController" docType="html-5.0" showHeader="false" >
	<apex:stylesheet value="{!URLFOR($Resource.mentoresd,'/mentoresd.css')}" />
	<style type="text/css">
		.colHeadr {text-align:center;} 
	</style>
	<apex:form >
		<apex:pageBlock title="Non-Employee Confirmation" id="theBlock" mode="detail">
			<apex:pageMessages id="msgs" />
			<apex:pageBlockSection title="Non-Employees Terminating in next 14 days" columns="1" id="oneTable" >
				<apex:PageBlockTable value="{!nonemployeesTransactional}" var="one"  >
				
					<apex:column headerValue="Name"  style="{!IF(one.LastModifiedBy.Name == 'Non Employee Attestation Site Guest User', 'Background-color: grey;', '')}">
	
						<apex:outputText >{!one.First_Name__c} {!one.Last_Name__c}</apex:outputText> 
					</apex:column>
					<apex:column >
						<apex:facet name="header">Job title</apex:facet>
						<apex:outputText >{!one.Job_Title__c}</apex:outputText> 
					</apex:column>
					<apex:column >
						<apex:facet name="header">Last Day</apex:facet>
						<apex:outputText Value=" {!one.Last_Day__c}"/>
					</apex:column>
					<apex:column headerClass="colHeadr" styleClass="colHeadr">
						<apex:facet name="header">Extend</apex:facet>
						
						<apex:commandButton value="30 Days" action="{!updateTMNUser}" reRender="theBlock,msgs">
							<apex:param value="{!one.Id}" assignTo="{!employeeId}" name="employeeId"/>
							<apex:param value="thirty" assignTo="{!flag}" name="flag"/>
						</apex:commandButton>
						&nbsp;
						<apex:commandButton value="60 Days" action="{!updateTMNUser}" reRender="theBlock,msgs" >
							<apex:param value="{!one.Id}" assignTo="{!employeeId}" name="employeeId"/>
							<apex:param value="sixty" assignTo="{!flag}" name="flag"/>
						</apex:commandButton>
						&nbsp;
						<apex:commandButton value="90 Days" action="{!updateTMNUser}" reRender="theBlock,msgs" >
							<apex:param value="{!one.Id}" assignTo="{!employeeId}" name="employeeId"/>
							<apex:param value="ninety" assignTo="{!flag}" name="flag"/>
						</apex:commandButton>
						&nbsp;
						<apex:input type="date" value="{!lastDay}" />
					</apex:column>
					
					
					<apex:column headerValue="Terminate On" headerClass="colHeadr" styleClass="colHeadr">
						<apex:commandButton value=" {!Today()}" action="{!updateTMNUser}" reRender="theBlock,msgs" >
							<apex:param value="{!one.Id}" assignTo="{!employeeId}" name="employeeId"/>
							<apex:param value="now" assignTo="{!flag}" name="flag"/>
						</apex:commandButton>
						&nbsp;
						<apex:commandButton value=" {!one.Last_Day__c}" action="{!updateTMNUser}" reRender="theBlock,msgs" >
							<apex:param value="{!one.Id}" assignTo="{!employeeId}" name="employeeId"/>
							<apex:param value="termDate" assignTo="{!flag}" name="flag"/>
						</apex:commandButton>
					</apex:column>
					
				</apex:PageBlockTable>
