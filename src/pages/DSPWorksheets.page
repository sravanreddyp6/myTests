<apex:page controller="TxnQueueResponseController" extensions="DSP_Worksheets" sidebar="false" tabStyle="ESD_Home_DSP__tab">
<apex:stylesheet value="{!URLFOR($Resource.mentoresd,'/mentoresd.css')}" />
<apex:stylesheet value="{!URLFOR($Resource.mentoresd,'/mentoresdcss2.css')}" />
<c:OneApp_PersonCard RequestInfo="{!RequestType + ',' + InputParams}" />

<apex:pageBlock title="Worksheets">
<apex:repeat value="{! apworksheets.mapSheets }" var="stype">

<a name="#{!stype}" /><apex:pageBlockSection title="{! stype}">

<apex:datatable value="{! apWorksheets.mapSheets[stype]}" var="w">
<apex:column headervalue="Action">
</apex:column>
<apex:column headerValue="Start Date">
<apex:outputText value="Week of {!w.startdate}"  rendered="{!stype == 'Skill'}"/><br />
<apex:outputText value=" {!w.startdate}"  rendered="{!stype == 'Behavior'}"/><br />
</apex:column>
</apex:datatable>
</apex:pageBlockSection>

</apex:repeat>
</apex:pageBlock>

</apex:page>