				modal: true,
				autoOpen: false,
				buttons: {
					Yes: function() {
						jQuery(this).dialog("close");
						isModified = false;
						window.afterRefresh = function () {
							// We use this to make sure that yesClicked is only triggered *after*
							// the postback is complete. Otherwise we'll run into a race situation
