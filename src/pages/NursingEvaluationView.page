			                    anno.reset();
			                    var nel = $('#neurobodyMap').get(0);
			                    if (nel) {
			                        anno.makeAnnotatable(nel);
			                    }
			                    anno.hideSelectionWidget();
			                    var $nel = $(nel);
			                    <apex:repeat value="{!neuroAnnotations}" var="neuroannotation">
			                    anno.addAnnotation({
			                        Id: "{!neuroannotation.Id}",
			                        src: $("#neurobodyMap").prop("src"),
			                        text: "{!JSENCODE(neuroannotation.Annotation_Text__c)}",
			                        shapes: [{
			                            type: "{!JSENCODE(neuroannotation.Annotation_Shape__c)}",
			                            geometry: {
			                                x: {!neuroannotation.Annotation_X__c},
			                                y: {!neuroannotation.Annotation_Y__c},
			                                width: {!neuroannotation.Annotation_Width__c},
			                                height: {!neuroannotation.Annotation_Height__c}
			                            }
			                        }],
			                        editable: false
			                        
			                    });
			                    </apex:repeat>
			                    var neurocreateAnnotationElements = function (neuroAnnotations) {
			                        $('.neuroannotation-number').remove();
			                        $('table#neuroAnnotations tr').remove();
			                        $.each(neuroAnnotations, function (index, neuroannotation) {
			                            var $divEl = $("<div class='neuroannotation-number' style='pointer-events:none; position: absolute; text-align: center; left: " + neuroannotation.shapes[0].geometry.x * 100 + "%; width: " + neuroannotation.shapes[0].geometry.width * 100 + "%; top: " + neuroannotation.shapes[0].geometry.y * 100 + "%; height: " + neuroannotation.shapes[0].geometry.height * 100 + "%;'></div>");
			                            $nel.parent().append($divEl);
			                            var $textEl = $("<span style='line-height: " + $divEl.height() + "px; color: red; font-size: 1.5em;'>" + (index + 1) + "</span>");
			                            $divEl.append($textEl);
			                            $("table#neuroAnnotations tbody").append("<tr><td>" + (index + 1) + "</td><td>" + neuroannotation.text + "</td></tr>");
			                        });
			                    }
			                    neurocreateAnnotationElements(anno.getAnnotations());
			                    anno.addHandler('onAnnotationCreated', function (neuroannotation) {
			                        neurocreateAnnotationElements(anno.getAnnotations());
			                    });
			                    anno.addHandler('onAnnotationUpdated', function (neuroannotation) {
			                        neurocreateAnnotationElements(anno.getAnnotations());
			                    });
			                    anno.addHandler('onAnnotationRemoved', function (neuroannotation) {
			                        neurocreateAnnotationElements(anno.getAnnotations());
			                    });
			                });
