                var PBRId = '{!Referral__c.Person_Being_Referred__c}';
                var PBSId = '{!Referral__c.Person_Being_Served__c}';


                if(PBRId != ''){
                    referral_Controller_Helper.saveDiagnosis(diagnosisId, status, effectiveTime, ICD,
                        PBRId,
                        function(result, event) {
                        	if(event.status){
                        		saveDiagnosis();
                        		if (isDialogClose) {
                                closeModalDialogR('diagnosisModal', 'span');
                            	}
                        	}else if (event.type === 'exception') {
								document.getElementById("diagnosisErrors").innerHTML = event.message;
							}else {
							document.getElementById("diagnosisErrors").innerHTML = event.message;
							}
                        }, {
                            escape: true
                        }
                    );
                }
                else if(PBSId != ''){
                    referral_Controller_Helper.saveDiagnosisFromPbsID(diagnosisId, status, effectiveTime, ICD,
                        PBSId,
                        function(result, event) {
                        	if(event.status){
                        		saveDiagnosis();
                            	if (isDialogClose) {
                                	closeModalDialogR('diagnosisModal', 'span');
                            	}
                        	}else if (event.type === 'exception') {
								document.getElementById("diagnosisErrors").innerHTML = event.message;
							} else {
								document.getElementById("diagnosisErrors").innerHTML = event.message;
							}
                        }, {
                            escape: true
                        }
                    );

                }
            }
            return false;
        }

