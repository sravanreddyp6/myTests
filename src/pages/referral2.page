        function validateadmissionLocation(){
            var errors=[];
            j$('#admissionLocationErrors').html(errors);
            var lookupId = '{!$Component.referralForm:ReferralAdmissionLocationModalBlock:admissionLocationEntry:admSectionItem:al:comp:lookupElement_lkid}';
            lookupId = lookupId.replace(/:/g, '\\:');
            lookupId = '#' +lookupId;

            var admDate = j$('input[id$=antAdmDate]').val();
            var aliasId = j$(lookupId).val();


            if(admDate.length == 0){
                errors.push('Please Choose an Admission Date');
            }

            if(aliasId.length == 0){
                errors.push('Please Choose an Admission Location');
            }

            if(errors.length > 0 ){
                j$.each(errors, function(index, error) {

                        j$('#admissionLocationErrors').html('<li>' +error+ '</li>');
                    });
            }else{

                saveAdmissionLocation();
            }


        }

        function getToday() {
			var today = new Date();
			var dd = today.getDate();
			var mm = today.getMonth() + 1; //January is 0!
			var yyyy = today.getFullYear();
			if (dd < 10) {
				dd = '0' + dd
			}
			if (mm < 10) {
				mm = '0' + mm
			}
			today = mm + '/' + dd + '/' + yyyy;
			return today;
		}
    </script>

