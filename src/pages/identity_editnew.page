                        <apex:selectOption itemValue="Redwood" itemLabel="Redwood"/>
                    </apex:selectList>
                    <apex:inputField label="Person Type" value="{!TMNUser.Current_Person_Type__c}" required="true"/>
                    <apex:inputField label="Job Status" value="{!TMNUser.Job_Status__c}" required="true"/>
                    <apex:selectList label="Pay Basis" value="{!TMNUser.Pay_Basis__c}" size="1" required="true">
                        <apex:selectOption itemValue="Hourly" itemLabel="Hourly"/>
                        <apex:selectOption itemValue="Salaried" itemLabel="Salaried"/>
                    </apex:selectList>                                                                                                                    
                    <apex:inputField label="Start Date" value="{!TMNUser.Hire_Date__c}" required="true"/>
