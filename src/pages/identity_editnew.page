                        <apex:selectOption itemValue="ID" itemLabel="Idaho"/>
                        <apex:selectOption itemValue="IL" itemLabel="Illinois"/>
                        <apex:selectOption itemValue="IN" itemLabel="Indiana"/>
                        <apex:selectOption itemValue="IA" itemLabel="Iowa"/>
