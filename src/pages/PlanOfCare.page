	<body>
<apex:messages style="color: red;"/>

<apex:outputPanel layout="none" rendered="{!!errorPresent}">
		<div class="pageContent" id="b1" >
			<table class="pageHeader" style="width: 100%;">
				<thead>
					<tr>
						<td class="labelCol" style="mso-protection: locked; width: 30%; font-size: 80%;">{!pbsObj.Service_Assignment__r[0].Name}</td>
						<td class="labelCol" style="text-align: center; vertical-align: top;">
                                <apex:outputLabel value="Plan of Care"/> <br/>
                                <apex:outputLabel value="(Authorized Service Plan Addendum)"/></td>
						<td style="width: 30%; text-align: right;">
						</td>
					</tr>
				</thead>
			</table>
            <table class="reportHeader" style="width: 100%;">
                <thead class="reportHeaderTableHeader">
                    <tr>
                        <td class="labelCol" style="text-align: center;"><apex:outputLabel value="Date of Plan of Care meeting:__/__/____" /></td>
                    </tr>
                    <tr>
                        <td class="labelCol" style="text-align: center;"><apex:outputLabel value="Effective Date of Approved Plan of Care:__/__/____" /></td>
                    </tr>
                </thead>
            </table>
			
            <br/>
            <apex:outputPanel layout="none">
			  <table style="width: 100%" >
                    <tr>
    				    <th colspan="2" class="risk-section-subhead">Services Needs/Support</th>
				    </tr>
                <apex:outputPanel rendered="{!assessment.Dsc_Prevocational_Services__c != null}" layout="none" >
				    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Prevocational</td>
				        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Prevocational_Services__c}"/></td>
				    </tr>
			     </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Day_Habilitation_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Day Habilitation</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Day_Habilitation_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Organizational_Employment_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Organizational Employment</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Organizational_Employment_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_School_Transition_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">School Transition</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_School_Transition_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!assessment.Dsc_Private_Pay_Services__c != null}" layout="none" >
                    <tr>
                        <td style="font: bold; width: 25%; padding-top: .5em;">Private Pay</td>
                        <td style="padding-left: .5em; text-align: justify;" ><apex:outputText value="{!assessment.Dsc_Private_Pay_Services__c}"/></td>
                    </tr>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!AND(ISBLANK(assessment.Dsc_Private_Pay_Services__c), ISBLANK(assessment.Dsc_School_Transition_Services__c), ISBLANK(assessment.Dsc_Organizational_Employment_Services__c),
                                                  ISBLANK(assessment.Dsc_Day_Habilitation_Services__c), ISBLANK(assessment.Dsc_Prevocational_Services__c)) }" layout="none" >
                    <tr>
                        <td colspan="2" style="font: bold; padding-top: .5em;">None</td>
                    </tr>
                </apex:outputPanel>
              </table>                
            </apex:outputPanel>

            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Staffing/Supervision Needs</th>
                    </tr>
                    <apex:repeat value="{!Staffing_Supervision_Needs}" var="item">
                        <tr>
                            <td style="padding-top: .5em;"><apex:outputText value="{!item}"/></td>
                        </tr>
                    </apex:repeat>
                </table>
            </apex:outputPanel> 

            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th colspan="2" class="risk-section-subhead">Comprehensive Functional Assessment Review</th>
                    </tr>
                    <tr>
                        <td style="font: bold; width: 30%;padding: .5em;text-align: right;"><apex:outputText value="Current strengths include but are not limited to"/></td>
                        <td>&nbsp;<br/>&nbsp;<br/>&nbsp;<br/>&nbsp;<br/>&nbsp;</td>
                    </tr>
                    <tr>
                        <td style="font: bold; width: 30%;padding: .5em;text-align: right;"><apex:outputText value="Current Needs include but not limited to"/></td>
                        <td>&nbsp;<br/>&nbsp;<br/>&nbsp;<br/>&nbsp;<br/>&nbsp;</td>
                    </tr>
                </table>
            </apex:outputPanel>            
            
            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Summary of Consumer Incident Reports</th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;"><apex:outputText value="Please see Incident Report Attachment."/></td>
                    </tr>
                </table>
            </apex:outputPanel>

            <br clear="all" style="page-break-before:always" />
            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Goals (Goals the team determined is appropriate)</th>
                    </tr>
                    <tr>
                        <td style="width: 100%">
		                    <apex:repeat value="{!planStuff}" var="g">    
		                    <table class="mytbl">
		                    <tr>
		                        <td style="width: 120px;">        
		                            <apex:outputText ><b>Goal</b></apex:outputText>
		                        </td>                
		                        <td>
		                            <apex:outputPanel layout="block" style="padding-bottom:20px">
		                                <apex:outputText value="{!planStuff[g].theGoal.Description__c}" />
		                            </apex:outputPanel>            
		                        </td>            
		                    </tr>
		                            <apex:repeat value="{!planStuff[g].GoalObjs}" var="o" >
		                    <tr>
		                                    <td style="text-indent: 1em;">
		                                        <apex:outputText ><b>Objective</b></apex:outputText>
		                                    </td>                    
		                                    <td>
		                                        <apex:outputPanel layout="block" id="theObj" style="padding-bottom:20px" styleClass="newObjClass">
		                                            <apex:outputText value="{!planStuff[g].GoalObjs[o].theObj.Description__c}"/> 

                                                    <table style="width: 100%">
                                                    <tr>
	                                                    <td class="innerLabel" style="width: 10%"><apex:outputLabel value="Status" /></td>
	                                                    <td style="width: 12%"><apex:outputText value="{!planStuff[g].GoalObjs[o].theObj.Status__c}" /></td>
	                                                    <td class="innerLabel" style="width: 25%" ><apex:outputLabel value="Effective Date" /></td>
	                                                    <td style="width: 14%"><apex:outputText value=" {!planStuff[g].GoalObjs[o].theObj.Effective_Date__c}" /></td>
	                                                    <td class="innerLabel" style="width: 25%"><apex:outputLabel value="Target Date" /></td>
	                                                    <td style="width: 14%"><apex:outputText value=" {!planStuff[g].GoalObjs[o].theObj.Target_End_Date__c}" /></td>
                                                    </tr>
                                                    </table> 
		                                                                
		                                        </apex:outputPanel>
		                                    </td>
		                    </tr>                
		                                    <apex:repeat value="{!planStuff[g].GoalObjs[o].ObjActs}" var="a" id="actsthe">
		                    <tr class="action_row">
		                                            <td style="text-indent: 2em;">
		                                            <apex:outputText ><b>Action</b></apex:outputText>                
		                                            </td> 
		                                            <td>
		                                                <apex:outputPanel layout="block" id="theAction" style="padding-bottom:20px">
		                                                    <apex:outputField value="{!planStuff[g].GoalObjs[o].ObjActs[a].theAction.Description__c}"/>                                          
		                                                </apex:outputPanel>
		                                            </td>
		                    </tr>                    
		                                </apex:repeat> <!-- end action-->
		                        </apex:repeat> <!-- end obj -->
		                        </table>
		                    </apex:repeat>
                    
                        </td>
                    </tr>
                </table>
            </apex:outputPanel> 
            
            <br clear="all" style="page-break-before:always" />
            <br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Medical information/Supports</th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%" >
			                    <tr>
			                        <th style="padding-left: .5em; text-align: left;" colspan="2">Does the individual have any specific dietary needs?</th>
			                    </tr>
                                <tr>
			                        <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}">
			                            <td style="vertical-align: top; width: 30%; padding-left: 1em; "> <input type="checkbox" checked="checked" /><apex:outputText value="Presents a Risk" /></td>
			                            <td style="vertical-align: top;"> <input type="checkbox" /><apex:outputText value="Does Not Present a Risk" /></td>
			                        </apex:outputPanel>
			                        <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Does Not Present a Risk'}">
			                            <td style="vertical-align: top; width: 30%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="Presents a Risk" /></td> 
			                            <td style="vertical-align: top;"><input type="checkbox" checked="checked" disabled="disabled"/><apex:outputLabel value="Does Not Present a Risk" /></td>
			                        </apex:outputPanel>
                                </tr>
                            </table>

                        </td>
                    </tr>
                    
            <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}">
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">If 'Presents a Risk', do the dietary needs address a significant health or safety risk to the individual?</th>
                                </tr>
                                <tr>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Description_of_Risk__c == 'Yes, refer to DAO for instructions and completion'}" >
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; ">
                                        <input type="checkbox" checked="checked" > Yes, refer to DAO for instructions and completion </input><br />
                                        <input type="checkbox" >No, DAO not required per IDT recommendation </input></td>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Description_of_Risk__c == 'No, DAO not required per IDT recommendation'}" >
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; ">
                                        <input type="checkbox"  > Yes, refer to DAO for instructions and completion </input><br />
                                        <input type="checkbox" checked="checked" >No, DAO not required per IDT recommendation </input></td>
                                    </apex:outputPanel>
                                </tr>
                            </table>

                        </td>
                    </tr>
            </apex:outputPanel>

                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;" colspan="2">Does the Individual require PRN During Day Programming Hours?</th>
                                </tr>
                                <tr>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Presents a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; "> <input type="checkbox" checked="checked" /><apex:outputText value="Presents a Risk" /></td>
                                        <td style="vertical-align: top;"> <input type="checkbox" /><apex:outputText value="Does Not Present a Risk" /></td>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Rating__c == 'Does Not Present a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="Presents a Risk" /></td> 
                                        <td style="vertical-align: top;"><input type="checkbox" checked="checked" disabled="disabled"/><apex:outputLabel value="Does Not Present a Risk" /></td>
                                    </apex:outputPanel>
                                </tr>
                            </table>

                        </td>
                    </tr>
                    
            <apex:outputPanel layout="none" rendered="{!responseMap['Health Conditions/Medical Treatment~Dietary Needs'].Rating__c == 'Presents a Risk'}">
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;">If 'Presents a Risk', Describe signs or symptoms that would require the need for the PRN to be administered</th>
                                </tr>
                                <tr>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Description_of_Risk__c == 'Individual requests the medication'}" >
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; ">
                                        <input type="checkbox" checked="checked" >Individual requests the medication</input><br />
                                        <input type="checkbox" >CMA/MM observes the need based on the following symptoms:</input></td>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~PRN During Day Programming Hours'].Description_of_Risk__c == 'CMA/MM observes the need based on the following symptoms:'}" >
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; ">
                                        <input type="checkbox"  >Individual requests the medication</input><br />
                                        <input type="checkbox" checked="checked" >CMA/MM observes the need based on the following symptoms:</input></td>
                                    </apex:outputPanel>
                                </tr>
                            </table>

                        </td>
                    </tr>
            </apex:outputPanel>

                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;" colspan="2">Does this individual Self-Medicate?</th>
                                </tr>
                                <tr>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~Self-Medicate'].Rating__c == 'Presents a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; "> <input type="checkbox" checked="checked" /><apex:outputText value="Presents a Risk" /></td>
                                        <td style="vertical-align: top;"> <input type="checkbox" /><apex:outputText value="Does Not Present a Risk" /></td>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~Self-Medicate'].Rating__c == 'Does Not Present a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="Presents a Risk" /></td> 
                                        <td style="vertical-align: top;"><input type="checkbox" checked="checked" disabled="disabled"/><apex:outputLabel value="Does Not Present a Risk" /></td>
                                    </apex:outputPanel>
                                </tr>
                            </table>

                        </td>
                    </tr>
                    
                    <tr>
                        <td style="padding-top: 1.5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;" colspan="2">Can this individual transport medication to and from the program?</th>
                                </tr>
                                <tr>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~Self-Transportation of Medication'].Rating__c == 'Presents a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em; "> <input type="checkbox" checked="checked" /><apex:outputText value="Yes" /></td>
                                        <td style="vertical-align: top;"> <input type="checkbox" /><apex:outputText value="No" /></td>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="none" rendered="{!responseMap['Medications~Self-Transportation of Medication'].Rating__c == 'Does Not Present a Risk'}">
                                        <td style="vertical-align: top; width: 30%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="Yes" /></td> 
                                        <td style="vertical-align: top;"><input type="checkbox" checked="checked" disabled="disabled"/><apex:outputLabel value="No" /></td>
                                    </apex:outputPanel>
                                </tr>
                            </table>
                        </td>
                    </tr>

                </table>
            </apex:outputPanel>

            <br /><br />
            <apex:outputPanel layout="none">
                <table style="width: 100%" >
                    <tr>
                        <th class="risk-section-subhead">Individual Rights Review <span style="vertical-align:sub">(This section will provide further detailed information on the individual's rights restrictions as noted in the authorized service plan)</span></th>
                    </tr>
                    <tr>
                        <td style="padding-top: .5em;">
                            <table style="width: 100%" >
                                <tr>
                                    <th style="padding-left: .5em; text-align: left;" colspan="3">Has this individual made any achievement towards their rights restriction restoration plans(lifting rights restrictions)?</th>
                                </tr>
                                <apex:outputPanel layout="none" rendered="{!assessment.Individual_Rights_Review__c == 'Yes'}">
                                <tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em; "> <input type="checkbox" checked="checked" /><apex:outputText value="Yes" /></td>
                                    <td rowspan="3" style="vertical-align: middle; width: 10%;"> <b>Explain</b></td>
                                    <td rowspan="3"> <apex:outputText value="{!assessment.Individual_Rights_Review_Explain__c}" /></td>
                                </tr><tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="No" /></td>
                                </tr><tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="N/A" /></td>
                                </tr> 
                                </apex:outputPanel>
                                
                                
                                <apex:outputPanel layout="none" rendered="{!assessment.Individual_Rights_Review__c == 'No'}">
                                <tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em; "> <input type="checkbox" /><apex:outputText value="Yes" /></td>
                                    <td rowspan="3" style="vertical-align: middle; width: 10%;"> <b>Explain</b></td>
                                    <td rowspan="3"> <apex:outputText value="{!assessment.Individual_Rights_Review_Explain__c}" /></td>
                                </tr><tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em;"><input type="checkbox" checked="checked"/><apex:outputLabel value="No" /></td>
                                </tr><tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em;"><input type="checkbox" /><apex:outputLabel value="N/A" /></td>
                                </tr> 
                                </apex:outputPanel>
                                
                                
                                <apex:outputPanel layout="none" rendered="{!assessment.Individual_Rights_Review__c == 'N/A'}">
                                <tr>
                                    <td style="vertical-align: top; width: 15%; padding-left: 1em; "> <input type="checkbox" /><apex:outputText value="Yes" /></td>
                                    <td rowspan="3" style="vertical-align: middle; width: 10%;"> <b>Explain</b></td>
